{
    "name"             : "formula9/framework",
    "description"      : "Core classes for the Formula Nine PHP Framework built on Silex 2.",
    "type"             : "library",
    "keywords"         : [ "PHP7", "MVC", "framework", "Formula9", "Silex" ],
    "license"          : "MIT",
    "authors"          : [
        {
            "name" : "Greg Truesdell",
            "email": "odd.greg@gmail.com"
        }
    ],
    "repositories": [

    ],
    "homepage"         : "https://github.com/Formula9/Framework",
    "support"          : {
        "issues": "https://github.com/Formula9/Framework/issues",
        "source": "https://github.com/Formula9/Framework"
    },
    "autoload"         : {
        "classmap"             : [
            "F9",
            "Nine",
            "tests"
        ],
        "exclude-from-classmap": [
            "Nine/Contracts/Container",
            "vendor/illuminate/console"
        ]
    },
    "minimum-stability": "stable",
    "require"          : {
        "php"                  : "^7.0.4",
        "formula9/core"        : "dev-master",
        "aura/sql"             : "^2.5",
        "aura/sqlquery"        : "^2.6",
        "illuminate/database"  : "~5.2",
        "illuminate/support"   : "~5.2",
        "illuminate/events"    : "~5.2",
        "illuminate/filesystem": "~5.2",
        "illuminate/view"      : "~5.2",
        "symfony/console"      : "~3.0.8",
        "symfony/form"         : "~3.1",
        "symfony/process"      : "~3.1",
        "symfony/security"     : "~3.1",
        "symfony/serializer"   : "~3.1",
        "symfony/stopwatch"    : "~3.1",
        "symfony/translation"  : "~3.1",
        "symfony/twig-bridge"  : "~3.1",
        "symfony/validator"    : "~3.1",
        "symfony/var-dumper"   : "~3.1",
        "symfony/yaml"         : "~3.1"
    },
    "require-dev"      : {
        "vlucas/phpdotenv": "^2.1@dev",
        "phpunit/phpunit" : "^5.1@dev",
        "tracy/tracy"     : "^2.4@dev",
        "psy/psysh"       : "@stable",
        "mockery/mockery" : "^0.9.4",
        "fzaninotto/faker": "^1.5"
    }
}
